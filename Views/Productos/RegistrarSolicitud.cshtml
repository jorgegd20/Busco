@model Solicitud

<h1>Registrar Solicitud</h1>

<form action="" method="post">
    <div class="form-group">
        <label asp-for="Producto"></label>
        <input asp-for="ProductoId" type="text" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="Descripcion"></label>
        <input asp-for="Descripcion" type="text" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="Comprador"></label>
        <input asp-for="CompradorId" type="text" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="Lugar"></label>
        <input asp-for="Lugar" type="text" class="form-control">
    </div>
    <button class="btn btn-primary">Registrar</button>
</form>